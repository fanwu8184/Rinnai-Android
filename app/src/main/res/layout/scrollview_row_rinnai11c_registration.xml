<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/framelayout_firemodel_tlrowheight">

        <LinearLayout
            android:id="@+id/linearlayout_firemodel_row"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="245"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView173"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="Fire model"
                    android:textColor="#8a000000"
                    android:textSize="@dimen/textview_network_name_textsize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="5"
                android:background="#FFFFFFFF"
                android:orientation="horizontal"></LinearLayout>
        </LinearLayout>
    </FrameLayout>

</LinearLayout>